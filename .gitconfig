[user]
  name = Sean McKenna
  email = mckennapsean@gmail.com
[core]
  editor = vim
  excludesfile = ~/.gitignore_global
[pager]
  branch = false
[push]
  default = current
[alias]
  in = init
  cl = clone
  st = status -sb
  di = diff --color-words
  co = commit -m
  ac = commit -am
  coa = commit --amend --no-edit
  cp = cherry-pick
  lo = log --oneline --abbrev-commit --graph --decorate --color
  lop = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %<|(80,trunc)%s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
  sh = stash
  shp = stash pop
  br = branch
  ch = checkout
  me = merge
  re = rebase
  rec = rebase --continue
  rem = rebase origin/master
  res = rebase master/stable
  fe = fetch
  pl = pull
  ps = push
  psf = push --force-with-lease
